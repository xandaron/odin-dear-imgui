	DragDropTargetId:                GuiID,
	DragDropTargetFullViewport:      GuiID,
	DragDropAcceptFlagsCurr:         GuiDragDropFlags,
	DragDropAcceptFlagsPrev:         GuiDragDropFlags,
	DragDropAcceptIdCurrRectSurface: f32,        // Target item surface (we resolve overlapping targets by prioritizing the smaller surface)
	DragDropAcceptIdCurr:            GuiID,      // Target item id (set at the time of accepting the payload)
	DragDropAcceptIdPrev:            GuiID,      // Target item id from previous frame (we need to store this to allow for overlapping drag and drop targets)
	DragDropAcceptFrameCount:        i32,        // Last time a target expressed a desire to accept the source
	DragDropHoldJustPressedId:       GuiID,      // Set when holding a payload just made ButtonBehavior() return a press.
	DragDropPayloadBufHeap:          Vector(u8), // We don't expose the ImVector<> directly, ImGuiPayload only holds pointer+size
	DragDropPayloadBufLocal:         [16]u8,     // Local buffer for small payloads

