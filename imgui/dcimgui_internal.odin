	// Drag and Drop
	DragDropActive:                  bool,
	DragDropWithinSource:            bool,       // Set when within a BeginDragDropXXX/EndDragDropXXX block for a drag source.
	DragDropWithinTarget:            bool,       // Set when within a BeginDragDropXXX/EndDragDropXXX block for a drag target.
