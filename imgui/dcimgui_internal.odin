	NavMoveResultOther:        GuiNavItemData, // Best move request candidate within NavWindow's flattened hierarchy (when using ImGuiWindowFlags_NavFlattened flag)
	NavTabbingResultFirst:     GuiNavItemData, // First tabbing request candidate within NavWindow and flattened hierarchy

	// Navigation: record of last move request
	NavJustMovedFromFocusScopeId:   GuiID, // Just navigated from this focus scope id (result of a successfully MoveRequest).
	NavJustMovedToId:               GuiID, // Just navigated to this id (result of a successfully MoveRequest).
	NavJustMovedToFocusScopeId:     GuiID, // Just navigated to this focus scope id (result of a successfully MoveRequest).
	NavJustMovedToKeyMods:          GuiKeyChord,
	NavJustMovedToIsTabbing:        bool,  // Copy of ImGuiNavMoveFlags_IsTabbing. Maybe we should store whole flags.
	NavJustMovedToHasSelectionData: bool,  // Copy of move result's ItemFlags & ImGuiItemFlags_HasSelectionUserData). Maybe we should just store ImGuiNavItemData.

	// Navigation: extra config options (will be made public eventually)
	// - Tabbing (Tab, Shift+Tab) and Windowing (Ctrl+Tab, Ctrl+Shift+Tab) are enabled REGARDLESS of ImGuiConfigFlags_NavEnableKeyboard being set.
	// - Ctrl+Tab is reconfigurable because it is the only shortcut that may be polled when no window are focused. It also doesn't work e.g. Web platforms.
	ConfigNavEnableTabbing:        bool,        // = true. Enable tabbing (Tab, Shift+Tab). PLEASE LET ME KNOW IF YOU USE THIS.
	ConfigNavWindowingWithGamepad: bool,        // = true. Enable Ctrl+Tab by holding ImGuiKey_GamepadFaceLeft (== ImGuiKey_NavGamepadMenu). When false, the button may still be used to toggle Menu layer.
	ConfigNavWindowingKeyNext:     GuiKeyChord, // = ImGuiMod_Ctrl | ImGuiKey_Tab (or ImGuiMod_Super | ImGuiKey_Tab on OS X). Set to 0 to disable. For reconfiguration (see #4828)
	ConfigNavWindowingKeyPrev:     GuiKeyChord, // = ImGuiMod_Ctrl | ImGuiMod_Shift | ImGuiKey_Tab (or ImGuiMod_Super | ImGuiMod_Shift | ImGuiKey_Tab on OS X)

