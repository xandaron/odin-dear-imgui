	LastBgColor:            u32,
	HostWindow:             ^GuiWindow,
	VisibleWindow:          ^GuiWindow,         // Generally point to window which is ID is == SelectedTabID, but when CTRL+Tabbing this can be a different window.
	CentralNode:            ^GuiDockNode,       // [Root node only] Pointer to central node.
	OnlyNodeWithWindows:    ^GuiDockNode,       // [Root node only] Set when there is a single visible node within the hierarchy.
	CountNodeWithWindows:   i32,                // [Root node only]
	LastFrameAlive:         i32,                // Last frame number the node was updated or kept alive explicitly with DockSpace() + ImGuiDockNodeFlags_KeepAliveOnly
	LastFrameActive:        i32,                // Last frame number the node was updated.
	LastFrameFocused:       i32,                // Last frame number the node was focused.
	LastFocusedNodeId:      GuiID,              // [Root node only] Which of our child docking node (any ancestor in the hierarchy) was last focused.
	SelectedTabId:          GuiID,              // [Leaf node only] Which of our tab/window is selected.
	WantCloseTabId:         GuiID,              // [Leaf node only] Set when closing a specific tab/window.
	RefViewportId:          GuiID,              // Reference viewport ID from visible window when HostWindow == NULL.
	AuthorityForPos:        GuiDataAuthority,
	AuthorityForSize:       GuiDataAuthority,
	AuthorityForViewport:   GuiDataAuthority,
	IsVisible:              bool,               // Set to false when the node is hidden (usually disabled as it has no active window)
	IsFocused:              bool,
